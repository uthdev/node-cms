{
  "name": "cms",
  "version": "1.0.0",
  "description": "A content management web app",
  "main": "index.js",
  "scripts": {
    "build": "babel -d ./build ./server -s",
    "clean": "rm -rf build",
    "prestart": "npm run clean && npm run build",
    "start": "node build/index",
    "start-dev": "nodemon --exec babel-node ./server/index.js",
    "test": "nyc --reporter=html --reporter=text mocha server/test/**/*.js --timeout 10000 --require @babel/register --recursive --exit",
    "coverage": "nyc report --reporter=text-lcov --reporter=lcov",
    "lint": "eslint --fix server/**/*.js"
  },
  "author": "Uthdev",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.5.5",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "connect-mongodb-session": "^2.3.1",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.5",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "supertest": "^4.0.2"
  }
}
